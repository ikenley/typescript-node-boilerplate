{
  "name": "typescript-node-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Typescript Node application",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "cli": "ts-node src/cli.ts",
    "dev": "tsc --project tsconfig.json --watch & NODE_ENV=development nodemon dist",
    "prestart": "npm run build",
    "start": "nodemon",
    "test": "jest --detectOpenHandles",
    "test-coverage": "jest --coverage",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist build",
    "ts-node": "ts-node"
  },
  "author": "ikenley",
  "license": "SEE LICENSE IN LICENSE.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikenley/typescript-node-boilerplate"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^27.5.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^17.0.45",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.4",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.883.0",
    "cognito-express": "^3.0.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.1.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.14",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.26",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  }
}
